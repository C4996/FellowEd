{
  "name": "fellowed",
  "displayName": "fellowed",
  "description": "结对编程好帮手",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fellowed.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "fellowed.scrollToLine",
        "title": "Scroll to Line"
      },
      {
        "command": "fellowed.start",
        "title": "Start"
      },
      {
        "command": "fellowed.showFileInfo",
        "title": "show the current open file's infomation"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "fastify": "^4.26.2",
    "zod": "^3.23.4"
  }
}
